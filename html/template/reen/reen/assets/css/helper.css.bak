*{
	margin:0;
	padding:0;
	box-sizing:border-box;
}
.container{
	margin:0 auto;
}.row{
	width:100%;
}
.row,
.row::after,
.row::before{
	display:table;
	clear:both;
}
.col-6,.col-2,.col-8,.col-4{
	float:left;
}
.col-6{
	width:calc((100%*6)/12);
}
.col-2{
	width:calc((100%*2)/12);
}
.col-8{
	width:calc((100%*8)/12);
}
.col-10{
	width:calc((100%*10)/12);
}
.col-4{
	width:calc((100% * 4) / 12);
}
.col-12{
	width:100%;
}
.alignLeft{
	text-align:left;
}
.alignRight{
	text-align:right;
}
.alignCenter{
	text-align:center;
}
ul,ol{
	list-style:none;
}
a{
	text-decoration:none;
}
.ml-oneGrid{
	margin-left:calc((100%/12));
}
.ml-2grid{
	margin-left:calc((2*100%) / 12);
}
.mb-80{
	margin-bottom:80px;
}